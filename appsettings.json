{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "OpenIddict.Core": "Warning",
        "OpenIddict.Server": "Warning",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "8e2fe196-3489-48fd-b924-103f4f6ab391",
        "SanitizeTinyMce": true,
        "ReservedPaths": "~/app_plugins/,~/install/,~/mini-profiler-resources/,~/umbraco/,~/umbraco-auto-login,~/api/,"
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      },
      "ModelsBuilder": {
        "IncludeVersionNumberInGeneratedModels": false,
        "ModelsMode": "Nothing",
        "ModelsNamespace": "UmbCacheBug.UmbracoModels",
        "ModelsDirectory": "~/UmbracoModels/"
      },
      "Unattended": {
        "InstallUnattended": true,
        "PackageMigrationsUnattended": true,
        "UpgradeUnattended": true,
        "UnattendedUserName": "user@example.com",
        "UnattendedUserEmail": "user@example.com",
        "UnattendedUserPassword": "password",
        "UnattendedTelemetryLevel": "Minimal"
      },
      "Security": {
        "AllowConcurrentLogins": true,
        "UserPassword": {
          "RequiredLength": 8
        }
      },
      "WebRouting": {
        "DisableRedirectUrlTracking": true
      }
    }
  },
  "uSync": {
    "Settings": {
      "ImportAtStartup": "Content,Settings",
      "ExportOnSave": "None",
      "RootFolder": "uSync"
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.Sqlite"
  }
}